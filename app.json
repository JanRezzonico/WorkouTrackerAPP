{
  "expo": {
    "name": "WorkouTracker",
    "slug": "workoutracker",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/wt-icon-bg.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#293237"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "This app needs access to your photo library to select pictures."
      },
      "bundleIdentifier": "com.workoutracker.workoutracker"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/wt-icon-bg.png",
        "backgroundColor": "#293237"
      },
      "package": "com.workoutracker.workoutracker",
      "permissions": [
        "CAMERA_ROLL"
      ]
    },
    "web": {
      "favicon": "./assets/wt-icon-bg.png"
    },
    "plugins": [
      [
        "expo-screen-orientation",
        {
          "initialOrientation": "PORTRAIT_UP"
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "0291bc93-1821-4b40-8e0a-3700be79a8ff"
      }
    },
    "owner": "workoutracker"
  }
}
